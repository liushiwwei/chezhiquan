/* 切换爱车 */

.cut {
  width: 750rpx;
  height: 122rpx;
  background-color: #f6f6f6;
  border-top-left-radius: 12rpx;
  border-top-right-radius: 12rpx;
  position: absolute;
  top: 362rpx;
}

.cut .cut_car {
  width: 722rpx;
  height: 75rpx;
  background-color: #fff;
  margin: 38rpx auto 0;
  display: flex;
  align-items: center;
}

.cut .cut_car text {
  font-size: 26rpx;
  line-height: 26rpx;
  margin-left: 20rpx;
}

.cut .cut_car image {
  width: 36rpx;
  height: 28rpx;
  margin-left: 33rpx;
}

/* 电池信息 */

.car_xinxi {
  width: 750rpx;
  height: 190rpx;
  background-color: #f6f6f6;
  margin-top: 120rpx;
  display: flex;
  justify-content: center;
  position: absolute;
  top: 364rpx;
}

.car_xinxi .dianchi {
  width: 337rpx;
  height: 100%;
  background-color: #fff;
}

.car_xinxi .dianchi .dian_chi {
  width: 67rpx;
  height: 108rpx;
  margin-left: 10rpx;
  margin-top: 40rpx;
  position: absolute;
}

.car_xinxi .dianchi .fute {
  margin-left: 110rpx;
  font-size: 55rpx;
  margin-top: 20rpx;
}

.car_xinxi .dianchi .fute .v {
  font-size: 25rpx;
}

.car_xinxi .dianchi .dianv {
  margin-left: 110rpx;
  position: absolute;
  bottom: 30rpx;
  font-size: 22rpx;
  line-height: 22rpx;
  color: #c9c9c9;
}

/* 今日里程信息 */

.car_xinxi .todaylu {
  width: 185rpx;
  height: 100%;
  background-color: #fff;
  margin: 0 8rpx;
}

.car_xinxi .todaylu .todaykm {
  font-size: 40rpx;
  color: #666;
  margin-top: 30rpx;
  margin-left: 20rpx;
}

.car_xinxi .todaylu .todaykm .km {
  font-size: 25rpx;
  color: #666;
  padding-left: 20rpx;
}

.car_xinxi .todaylu .tkm {
  font-size: 22rpx;
  line-height: 22rpx;
  color: #c9c9c9;
  margin-left: 20rpx;
  position: absolute;
  bottom: 30rpx;
}

/* 总里程信息 */

.car_xinxi .totallu {
  width: 185rpx;
  height: 100%;
  background-color: #fff;
}

.car_xinxi .totallu .totalkm {
  font-size: 40rpx;
  color: #666;
  margin-left: 20rpx;
  margin-top: 30rpx;
}

.car_xinxi .totallu .totalkm .total {
  font-size: 25rpx;
  color: #666;
  padding-left: 15rpx;
}
.car_xinxi .totallu  .tkm{
   font-size: 22rpx;
  line-height: 22rpx;
  color: #c9c9c9;
  margin-left: 20rpx;
  position: absolute;
  bottom: 30rpx;
}

