/* pages/callbj/callbj.wxss */
page {
  display: block;
  min-height: 100%;
  background-color: #f6f6f6;
}

.callbj {
  width: 750rpx;
  height: 120rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  margin-top: 5rpx;
}

.callbj .left {
  margin-left: 20rpx;
  flex-wrap: wrap;
  width: 270rpx;
  height: 120rpx;
  position: relative;
}

.callbj .left .text1 {
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333;
  position: absolute;
  top: 25rpx;
}

.callbj .left .text2 {
  font-size: 22rpx;
  line-height: 22rpx;
  color: #999;
  position: absolute;
  bottom: 22rpx;
}
.callbj .left .text4{
  font-size: 22rpx;
  line-height: 22rpx;
  color: #999;
  position: absolute;
  bottom: 22rpx;
  left: 150rpx;
}

.callbj .right{
  flex: 1;
  height: 120rpx;
  position: relative;
}

.callbj .right .text3{
  font-size: 32rpx;
  line-height: 32rpx;
  color: #333;
  position: absolute;
  top: 26rpx;
}

.callbj .right .text5{
  font-size: 23rpx;
  line-height: 23rpx;
  color: #999;
  position: absolute;
  bottom: 22rpx;
  right: 50rpx;
}