}

.mapo{
  width: 100%;
  height: 246rpx;
  background-color: rgba(0, 0, 0, 0.1);
  text-align: center;
}

.mapo .moo{
  width: 672rpx;
  height: 200rpx;
  background-color: #fff;
  border-radius: 8rpx;
  margin: 22rpx auto;  
  text-align: left;
  position: relative;
  font-size: 25rpx;
  line-height: 25rpx;
  padding-left: 28rpx;
}

.mapo .moo cover-image{
  width: 62rpx;
  height: 62rpx;
  position: absolute;
  right: 24rpx;
  top: 24rpx;
}

