}
#myMap {
  width: 100%;
  min-height:100%;
  position: relative;
}

#myMap image {
  width: 60rpx;
  height: 60rpx;
  margin-top: 530rpx;
  margin-left: 30rpx;
}

.cover-image {
  width: 80rpx;
  height: 80rpx;
  margin-left: 20rpx;
  position: absolute;
  bottom: 20rpx;
}

.gps {
  width: 750rpx;
  height: 120rpx;
  background-color: #1296db;
